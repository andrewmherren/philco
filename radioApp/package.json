{
    "name": "my-electron-app",
    "version": "0.1.0",
    "main": "main.js",
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "autoprefixer-stylus": "^0.14.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-react-transform": "^3.0.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-react-display-name": "^6.25.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.26.0",
        "cross-env": "^5.1.4",
        "css-loader": "^0.28.11",
        "electron-packager": "^12.0.1",
        "electron-rebuild": "^1.1.3",
        "eslint": ">=2.9.0",
        "eslint-config-standard": "^11.0.0",
        "eslint-plugin-babel": "^5.1.0",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-react": "^7.7.0",
        "eslint-plugin-standard": "^3.0.1",
        "file-loader": "^1.1.11",
        "image-webpack-loader": "^4.2.0",
        "less": "^3.0.2",
        "less-loader": "^4.1.0",
        "node-libs-browser": "^2.1.0",
        "postcss-loader": "^2.1.4",
        "poststylus": "^1.0.0",
        "react-transform-catch-errors": "^1.0.2",
        "react-transform-hmr": "^1.0.4",
        "redbox-react": "^1.5.0",
        "rimraf": "^2.5.4",
        "style-loader": "^0.21.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "url-loader": "^1.0.1",
        "webpack": "^4.6.0",
        "webpack-cli": "^2.0.15",
        "webpack-dev-server": "^3.1.3"
    },
    "scripts": {
        "start-electron": "./node_modules/.bin/electron .",
        "start": "./node_modules/.bin/webpack-dev-server --port 5000",
        "electron-rebuild": "./node_modules/.bin/electron-rebuild",
        "build": "cross-env npm run clean && npm run build:webpack",
        "clean": "cross-env rimraf ./public",
        "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
        "win64-packager": "./node_modules/.bin/electron-packager ./ my_app --platform=win32 --arch=x64 --ignore=node_modules --ignore=Client --ignore=webpack.* --ignore=README.* --asar",
        "win32-packager": "./node_modules/.bin/electron-packager ./ my_app --platform=win32 --arch=ia32 --ignore=node_modules --ignore=Client --ignore=webpack.* --ignore=README.* --asar",
        "linux64-packager": "./node_modules/.bin/electron-packager ./ my_app --platform=linux --arch=x64 --ignore=node_modules --ignore=Client --ignore=webpack.* --ignore=README.* --asar",
        "linux32-packager": "./node_modules/.bin/electron-packager ./ my_app --platform=linux --arch=ia32 --ignore=node_modules --ignore=Client --ignore=webpack.* --ignore=README.* --asar",
        "osx-packager": "./node_modules/.bin/electron-packager ./ my_app --platform=darwin --ignore=node_modules --ignore=Client --ignore=webpack.* --ignore=README.* --asar",
        "ios-packager": "./node_modules/.bin/electron-packager ./ my_app --platform=darwin --ignore=node_modules --ignore=Client --ignore=webpack.* --ignore=README.* --asar",
        "all-packager": "./node_modules/.bin/electron-packager ./ my_app --all --ignore=node_modules --ignore=Client --ignore=webpack.* --ignore=README.* --asar",
        "build-win64": "cross-env npm run build && npm run win64-packager",
        "build-win32": "cross-env npm run build && npm run win32-packager",
        "build-linux64": "cross-env npm run build && npm run linux64-packager",
        "build-linux32": "cross-env npm run build && npm run linux32-packager",
        "build-osx": "cross-env npm run build && npm run osx-packager",
        "build-ios": "cross-env npm run build && npm run ios-packager",
        "build-all": "cross-env npm run build && npm run all-packager"
    },
    "dependencies": {
        "electron-prebuilt": "^1.4.13",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "serialport": "^6.2.0",
        "socket.io": "^2.1.0"
    }
}
